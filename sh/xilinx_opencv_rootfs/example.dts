			xlnx,s00-axi-addr-width = <0x4>;
			xlnx,s00-axi-data-width = <0x20>;
		};
	};

	chosen {
		bootargs = "console=ttyPS0,115200 noinitrd root=/dev/mmcblk0p2 rw earlyprintk rootfstype=ext4 ip=192.168.1.212 rootwait";
		#bootargs = "console=ttyPS0,115200";
		linux,stdout-path = "/amba@0/serial@e0001000";
	};

	aliases {
		ethernet0 = "/amba@0/ps7-ethernet@e000b000";
		serial0 = "/amba@0/serial@e0001000";
		spi0 = "/amba@0/ps7-qspi@e000d000";
	};

	memory@00100000 {
		device_type = "memory";
		reg = <0x0 0x20000000>;
	};
};
