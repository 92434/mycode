			reg = <0xe1000000 0x1000000>;
			status = "disabled";
		};

		ps7-sd@0xe0101000 {
			clock-names = "ref_clk", "aper_clk";
			clocks = <0x2 0x16 0x2 0x21>;
			compatible = "xlnx,ps7-sd-1.00.a";
			interrupt-parent = <0x3>;
			interrupts = <0x0 0x2f 0x4>;
